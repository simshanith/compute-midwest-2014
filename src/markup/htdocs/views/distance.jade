.where-from
  .map-container(ng-controller='mapcontroller')
    // center="[39.0954135, -94.577938]"
    map(zoom="16" 
      disable-default-u-i="true"
      disable-double-click-zoom="true"
      draggable="false"
      draggable-cursor="none"
      dragging-cursor="move"
      keyboard-shortcuts="false"
      max-zoom="12"
      min-zoom="8"
      tilt="90"
      map-type-id="TERRAIN").squireMap
    .map-copy
      h2 Starting From
      h1 Overland Park
      a(href="#") Change
  
  h1 How Far Would You Like Travel
  .slider(distance='')
    input.rangeinput.bar(type='range', name='sliderBar',min='0',max='100')
    output(for="sliderBar",onforminput="value = sliderBar.value")
  h1 Who Are You With
  .friends.pure-g
    .friend.josh.pure-u-1-5
    .friend.add.pure-u-1-5
    .friend.blank
    .friend.blank
    .friend.blank
  .pure-button(ui-sref="categories")
