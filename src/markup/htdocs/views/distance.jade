.where-from
  .map-container(ng-controller='mapcontroller')
    // center="[39.0954135, -94.577938]"
    map(zoom="16" 
      disable-default-u-i="true"
      disable-double-click-zoom="true"
      draggable="false"
      draggable-cursor="none"
      dragging-cursor="move"
      keyboard-shortcuts="false"
      max-zoom="12"
      min-zoom="8"
      tilt="90"
      map-type-id="TERRAIN").squireMap
    .map-copy
      h1 Starting From
      h5 Overland Park
      a(href="#") Change
  .slider-pod
    h1 How Far Would You Like Travel
    .slider(distance='')
      span.before 5 mi
      input.rangeinput.bar(type='range', name='sliderBar',min='5',max='40')
      span.after 40+ mi
      //output(for="sliderBar",onforminput="value = sliderBar.value")
  .friend-pod
    h1 Who Are You With
    .friends.pure-g
      .friend.josh.pure-u-1-5
        img.pure-img(src="/assets/images/icons/josh-friend.png")
      .friend.add.pure-u-1-5
        img.pure-img(src="/assets/images/icons/empty-friend.png")
      .friend.blank
      .friend.blank
      .friend.blank
  .pure-button(ui-sref="categories")
    h1 next
